{
  "name": "package-engines-notifier",
  "description": "warns users about incompatible Node.js versions per engines in package.json",
  "version": "1.0.0",
  "author": "Ron Waldon <jokeyrhyme@gmail.com> (https://github.com/jokeyrhyme)",
  "dependencies": {
    "semver": "^5.3.0"
  },
  "devDependencies": {
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-node": "^2.1.3",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "fixpack": "^2.3.1",
    "flow-bin": "^0.33.0",
    "jest": "^16.0.2"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    }
  },
  "keywords": [
    "engines",
    "package",
    "package.json",
    "semver",
    "update-notifier",
    "version"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint --fix --cache .",
    "fixpack": "fixpack",
    "flow_check": "flow check",
    "jest": "jest",
    "posttest": "npm run eslint && npm run flow_check && npm run fixpack",
    "test": "npm run jest"
  }
}
